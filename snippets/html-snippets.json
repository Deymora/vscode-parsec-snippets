{
  "ParsecBtn": {
    "prefix": "pbtn",
    "body": [
      "<parsec-btn",
      "\t@click=\"${1:}\"",
      "\t:label=\"${2:$$t('')}\"",
      "\t:tooltip=\"${3:$$t('')}\"",
      "\ticon=\"${4:}\"",
      "/>"
    ],
    "description": "Parsec Button"
  },
  "ParsecBtnDropdown": {
    "prefix": "pbtndropdown",
    "body": [
      "<parsec-btn-dropdown",
      "\t:label=\"${1:$$t('')}\"",
      "\t:options=\"${2:options}\"",
      "\t@item-click=\"${3:}\"",
      ">",
      "\t<template #option-toolbar=\"{option}\">\"",
      "\t\t<parsec-icon",
      "\t\t\tname=\"icon-trash\"",
      "\t\t\t@click.stop=\"() => func(option)\"",
      "\t\t\t:label=\"${1:$$t('')}\"",
      "\t\t/>",
      "\t</template>",
      "</parsec-btn-dropdown>"
    ],
    "description": "Parsec Dropdown Button"
  }
}