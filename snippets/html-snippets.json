{
  "ParsecBtn": {
    "prefix": "pbtn",
    "body": [
      "<parsec-btn v-model=\"${1:header}\" ${2:reveal} ${3:elevated} ${4:bordered}>$0</q-header>"
    ],
    "description": "Parsec Button"
  },
  "QDrawer": {
    "prefix": "qdrawer",
    "body": [
      "<q-layout-drawer",
      "\tside=\"${1:left}\"",
      "\tv-model=\"${2:drawerLeft}\"",
      "\t${3:bordered}",
      "\t:width=\"${4:200}\"",
      "\t:breakpoint=\"${5:500}\"",
      "\tcontent-class=\"${6:bg-grey-3}\"",
      ">",
      "\t$0",
      "</q-layout-drawer>"
    ],
    "description": "QDrawer is the sidebar part of your QLayout."
  }
}
